{
    "name": "free2er/jwt-authenticator-bundle",
    "description": "JWT Authenticator Bundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Pryakhin",
            "email": "m.pryakhin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "lcobucci/jwt": "^3.2",
        "nesbot/carbon": "^2.16",
        "symfony/config": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/security-core": "^4.2",
        "symfony/security-guard": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Free2er\\Jwt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Free2er\\Jwt\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "free2er/coding-standard": "^1.0",
        "phpunit/phpunit": "^8.0"
    }
}
